<h1>ログイン</h1>
<% if flash[:danger] %>
  <p><%= flash[:danger] %></p>
<% end %>
  <%= form_for(:session, url: login_path) do |f| %>
    <p>
      <%= f.label :email,"メールアドレス" %>
      <%= f.text_field :email %>
    </p>
    <p>
      <%= f.label :password,"パスワード" %>
      <%= f.password_field :password %>
    </p>
     
    <%= f.submit "ログイン" %>
  <% end %>